<view class="pics"><scroll-view class="left" scroll-y="{{true}}"><view data-event-opts="{{[['tap',[['leftClickNav',[index,'$0'],[[['name','name',item.name]]]]]]]}}" class="{{((active==index)?'active':'')}}" onTap="__e" a:for="{{name}}" a:for-item="item" a:for-index="index" a:key="name">{{item.name}}</view></scroll-view><scroll-view class="right" scroll-y="{{true}}"><block a:for="{{items}}" a:for-item="item" a:for-index="__i0__" a:key="categoryId"><view data-event-opts="{{[['tap',[['details',['$0'],[[['items','categoryId',item.categoryId]]]]]]]}}" class="right-box" onTap="__e"><view class="right-boxs" style="width:100%;height:100%;"><text space="nbsp">{{item.name}}</text><view class="img"><image class="img-b" src="{{item.pics[0]}}"></image><image class="a" src="{{item.pics[1]}}"></image></view></view></view></block></scroll-view></view>